{
  "name": "wowify",
  "version": "1.0.1",
  "description": "A fun JavaScript library to create dynamic effects like confetti parties.",
  "main": "dist/wowify.cjs.js",
  "module": "dist/wowify.esm.js",
  "browser": "dist/wowify.min.js",
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "module",
    "url": "https://github.com/satodu/Wowify.js"
  },
  "keywords": [
    "javascript",
    "animations",
    "confetti",
    "effects",
    "fun"
  ],
  "author": "Panda <sato.du@gmail.com> (https://github.com/satodu)",
  "license": "MIT",
  "homepage": "https://github.com/satodu/Wowify.js#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "rollup": "^4.28.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "exports": {
    ".": {
      "require": "./dist/wowify.cjs.js",
      "import": "./dist/wowify.esm.js",
      "browser": "./dist/wowify.min.js"
    }
  }
